   "source": [
    "words = [\"hello\", \"world\"]\n",
    "b_dict = {}\n",
    "  \n",
    "for w in words:\n",
    "    chs = [\"<S>\"] + list(w) + [\"<E>\"]\n",
    "    for ch1, ch2 in zip(chs, chs[1:]):\n",
    "        biagram = ch1, ch2\n",
    "        b_dict[biagram] = b_dict.get(biagram, 0) + 1\n",
    "list(b_dict)    "
